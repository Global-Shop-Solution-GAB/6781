Program.Sub.LoadDtXStatus.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare

	V.Local.dCutoffDate.Declare
	V.Local.iFor.Declare
	V.Local.iMonth.Declare
	V.Local.sActiveFilterString.Declare
	V.Local.sCurrentMonth.Declare
	V.Local.sDate.Declare
	V.Local.sLast.Declare
	V.Local.sNext.Declare
	V.Local.sPart.Declare
	V.Local.sSQL.Declare

	Gui.frmXStatus.lblStatus.Visible(True)

	F.Intrinsic.Control.If(V.Global.bLoadDate)
		F.Intrinsic.Control.CallSub(Serialize)
	F.Intrinsic.Control.EndIf
	V.Global.bLoadDate.Set(True)

	F.Intrinsic.Control.If(V.DataTable.dtXStatus.Exists)
		F.Data.DataTable.Close("dtXStatus")
	F.Intrinsic.Control.EndIf

	'Determine the current month number in 00 format
	F.Intrinsic.String.Format(V.Ambient.Date,"MM",V.Local.sCurrentMonth)
	'Determine the last three months
	F.Intrinsic.Math.ConvertToLong(V.Local.sCurrentMonth,V.Local.iMonth)
	V.Local.sLast.Redim(1,6)
	F.Intrinsic.Control.For(V.Local.iFor,1,6,1)
		F.Intrinsic.Math.Sub(V.Local.iMonth,1,V.Local.iMonth)
		F.Intrinsic.Control.If(V.Local.iMonth,<=,0)
			F.Intrinsic.Math.Add(V.Local.iMonth,12,V.Local.iMonth)
		F.Intrinsic.Control.EndIf
		F.Intrinsic.String.Format(V.Local.iMonth,"00",V.Local.sLast(V.Local.iFor))
	F.Intrinsic.Control.Next(V.Local.iFor)
	'Determine the next three months
	F.Intrinsic.Math.ConvertToLong(V.Local.sCurrentMonth,V.Local.iMonth)
	V.Local.sNext.Redim(1,3)
	F.Intrinsic.Control.For(V.Local.iFor,1,3,1)
		F.Intrinsic.Math.Add(V.Local.iMonth,1,V.Local.iMonth)
		F.Intrinsic.Control.If(V.Local.iMonth,>,12)
			F.Intrinsic.Math.Sub(V.Local.iMonth,12,V.Local.iMonth)
		F.Intrinsic.Control.EndIf
		F.Intrinsic.String.Format(V.Local.iMonth,"00",V.Local.sNext(V.Local.iFor))
	F.Intrinsic.Control.Next(V.Local.iFor)
	'Determine 30 days ago
	F.Intrinsic.Date.DateAdd("D",-30,V.Ambient.Date,V.Local.sDate)
	F.Intrinsic.String.Format(V.Local.sDate,"yyyy-mm-dd",V.Local.sDate)


	F.Intrinsic.Control.If(V.Screen.frmXStatus!chkCutoff.Value,=,0)
		F.Intrinsic.String.Build("Select A.Part+A.Location As PartLoc,A.Part,A.Location,RTrim(A.Description) As Description, RTrim(RTrim(B.Description_2)+' '+RTrim(B.Description_3)) as AltDesc, RTrim(D.Text) As ExtraDesc,A.Product_Line,A.UM_Inventory,A.Time_Material_Lead,A.Code_Sort,RTrim(A.Bin) As Bin,A.Code_ABC,Cast(Null As Date) As Date_History,A.Qty_OnHand, A.QTY_ORDER, A.Qty_Reorder,A.Qty_OnOrder_PO+A.Qty_OnOrder_WO As OnOrder,A.Qty_Required, A.QTY_ONHAND + A.QTY_ONORDER_PO + A.QTY_ONORDER_WO - QTY_REQUIRED as Net, A.Qty_Usage_Mo_01+A.Qty_Usage_Mo_02+A.Qty_Usage_Mo_03+A.Qty_Usage_Mo_04+A.Qty_Usage_Mo_05+A.Qty_Usage_Mo_06+A.Qty_Usage_Mo_07+A.Qty_Usage_Mo_08+A.Qty_Usage_Mo_09+A.Qty_Usage_Mo_10+A.Qty_Usage_Mo_11+A.Qty_Usage_Mo_12 As Usage_Last_12_Mos, A.Qty_Usage_Mo_{0}+A.Qty_Usage_Mo_{1}+A.Qty_Usage_Mo_{2} As Usage_Last_3_Mos, A.Qty_Usage_Mo_{0}+A.Qty_Usage_Mo_{1}+A.Qty_Usage_Mo_{2}+A.Qty_Usage_Mo_{6}+A.Qty_Usage_Mo_{7}+A.Qty_Usage_Mo_{8} As Usage_Last_6_Mos, Cast(0 As Numeric(12,4)) As Usage_30, Cast(0 As Numeric(12,4)) As Usage_60, A.Qty_Usage_Mo_{3}+A.Qty_Usage_Mo_{4}+A.Qty_Usage_Mo_{5} As Usage_Next_3_Mos,A.Amt_Price, A.Amt_Alt_Cost, C.Cost, C.Matl, C.Labor, C.Ovhd, C.Outs, C.Freight, C.Oth,A.Qty_OnHand*A.Amt_Price As Ext_Price,A.Qty_OnHand*C.Cost As Ext_Cost,Cast(Case When Flag_Lot = '' Or Flag_Lot = 'N' Then 0 Else 1 End As Bit) As Flag_Lot,Cast(Case When Flag_Drop = '' Or Flag_Drop = 'N' Then 0 Else 1 End As Bit) As Flag_Drop,Cast(Case When Flag_Serialize = '' Or Flag_Serialize = 'N' Then 0 Else 1 End As Bit) As Flag_Serialize,Cast(Case When Obsolete_Flag = '' Or Obsolete_Flag = 'N' Then 0 Else 1 End As Bit) As Obsolete_Flag,Cast(Case When Flag_Inactive = '' Or Flag_Inactive = 'N' Then 0 Else 1 End As Bit) As Flag_Inactive, A.TEXT_INFO1 From V_Inventory_ALL A Join V_Inventory_Mst2 B On A.Part = B.Part And A.Location = B.Location Join V_Inventory_Cost C On A.Part = C.Part And A.Location = C.Locn Left Join V_Inv_Extra_Text D On A.Part = D.Part Order By A.Part,A.Location;",V.Local.sLast(1),V.Local.sLast(2),V.Local.sLast(3),V.Local.sNext(1),V.Local.sNext(2),V.Local.sNext(3),V.Local.sLast(4),V.Local.sLast(5),V.Local.sLast(6),V.Local.sSQL)
		F.Data.DataTable.CreateFromSQL("dtXStatus","con",V.Local.sSQL,True)
		'30 day usage
		F.Intrinsic.String.Build("select PART+LOCATION as PartLoc, sum(-Quantity) as Usage from V_INVENTORY_HIST where CODE_TRANSACTION in ('J55','O35')and DATE_HISTORY >= '{0}' group by PartLoc",V.Local.sDate,V.Local.sSQL)
		F.Data.Dictionary.CreateFromSQL("d30Day","con",V.Local.sSQL)
		F.Data.Dictionary.SetDefaultReturn("d30Day",0)
		F.Data.DataTable.FillFromDictionary("dtXStatus","d30Day","PartLoc","Usage_30")
		F.Data.Dictionary.Clear("d30Day")
		F.Data.Dictionary.Close("d30Day")
		'60 day usage
		'Determine 60 days ago
		F.Intrinsic.Date.DateAdd("D",-60,V.Ambient.Date,V.Local.sDate)
		F.Intrinsic.String.Format(V.Local.sDate,"yyyy-mm-dd",V.Local.sDate)
		F.Intrinsic.String.Build("select PART+LOCATION as PartLoc, sum(-Quantity) as Usage from V_INVENTORY_HIST where CODE_TRANSACTION in ('J55','O35')and DATE_HISTORY >= '{0}' group by PartLoc",V.Local.sDate,V.Local.sSQL)
		F.Data.Dictionary.CreateFromSQL("d60Day","con",V.Local.sSQL)
		F.Data.Dictionary.SetDefaultReturn("d60Day",0)
		F.Data.DataTable.FillFromDictionary("dtXStatus","d60Day","PartLoc","Usage_60")
		F.Data.Dictionary.Clear("d60Day")
		F.Data.Dictionary.Close("d60Day")
	F.Intrinsic.Control.Else
		V.Local.dCutoffDate.Set(V.Screen.frmXStatus!dtpCutoff.Value)
		'Find Part+Loc with data after the Cutoff
		F.Intrinsic.String.Build("Select Distinct Part+Location From V_Inventory_Hist Where Date_History > '{0}' And Part <> '' And SubString(Part,17,1) <> '*' Order By Part,Location;",V.Local.dCutoffDate.PervasiveDate,V.Local.sSQL)
		F.ODBC.Connection!con.ExecuteAndReturn(V.Local.sSQL,V.Local.sPart)
		F.Intrinsic.String.Replace(V.Local.sPart,"*!*","",V.Local.sPart)
		F.Intrinsic.String.Replace(V.Local.sPart,"#$#","','",V.Local.sPart)
		'Get information for the Part+Loc above
