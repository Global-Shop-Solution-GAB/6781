Program.Sub.LoadTotals.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare

	V.Local.fTotal.Declare
	V.Local.sTotal.Declare
	V.Local.sFilter.Declare

	F.Intrinsic.Control.SelectCase(V.Args.Load)
		F.Intrinsic.Control.Case("First")
				'Qty_OnHand
				F.Data.DataTable.Compute("dtXStatus","Sum(Qty_OnHand)","",V.Local.fTotal)
				F.Intrinsic.String.Format(V.Local.fTotal,"#,###,###,##0.00####",V.Local.sTotal)
				Gui.frmXStatus.txtQtyOnHand.Text(V.Local.sTotal)
				'OnOrder
				F.Data.DataTable.Compute("dtXStatus","Sum(OnOrder)","",V.Local.fTotal)
				F.Intrinsic.String.Format(V.Local.fTotal,"#,###,###,##0.00####",V.Local.sTotal)
				Gui.frmXStatus.txtQtyOnOrder.Text(V.Local.sTotal)
				'Qty_Required
				F.Data.DataTable.Compute("dtXStatus","Sum(Qty_Required)","",V.Local.fTotal)
				F.Intrinsic.String.Format(V.Local.fTotal,"#,###,###,##0.00####",V.Local.sTotal)
				Gui.frmXStatus.txtQtyReqd.Text(V.Local.sTotal)
				'Ext_Price
				F.Data.DataTable.Compute("dtXStatus","Sum(Ext_Price)","",V.Local.fTotal)
				F.Intrinsic.String.Format(V.Local.fTotal,"#,###,###,##0.00####",V.Local.sTotal)
				Gui.frmXStatus.txtExtPrice.Text(V.Local.sTotal)
				'Ext_Cost
				F.Data.DataTable.Compute("dtXStatus","Sum(Ext_Cost)","",V.Local.fTotal)
				F.Intrinsic.String.Format(V.Local.fTotal,"#,###,###,##0.00####",V.Local.sTotal)
				Gui.frmXStatus.txtExtCost.Text(V.Local.sTotal)
		F.Intrinsic.Control.Case("Filter")
				'Qty_OnHand
				F.Data.DataTable.Compute("dtXStatus","Sum(Qty_OnHand)",V.Args.Filter,V.Local.fTotal)
				F.Intrinsic.String.Format(V.Local.fTotal,"#,###,###,##0.00####",V.Local.sTotal)
				Gui.frmXStatus.txtQtyOnHand.Text(V.Local.sTotal)
				'OnOrder
				F.Data.DataTable.Compute("dtXStatus","Sum(OnOrder)",V.Args.Filter,V.Local.fTotal)
				F.Intrinsic.String.Format(V.Local.fTotal,"#,###,###,##0.00####",V.Local.sTotal)
				Gui.frmXStatus.txtQtyOnOrder.Text(V.Local.sTotal)
				'Qty_Required
				F.Data.DataTable.Compute("dtXStatus","Sum(Qty_Required)",V.Args.Filter,V.Local.fTotal)
				F.Intrinsic.String.Format(V.Local.fTotal,"#,###,###,##0.00####",V.Local.sTotal)
				Gui.frmXStatus.txtQtyReqd.Text(V.Local.sTotal)
				'Ext_Price
				F.Data.DataTable.Compute("dtXStatus","Sum(Ext_Price)",V.Args.Filter,V.Local.fTotal)
				F.Intrinsic.String.Format(V.Local.fTotal,"#,###,###,##0.00####",V.Local.sTotal)
				Gui.frmXStatus.txtExtPrice.Text(V.Local.sTotal)
				'Ext_Cost
				F.Data.DataTable.Compute("dtXStatus","Sum(Ext_Cost)",V.Args.Filter,V.Local.fTotal)
				F.Intrinsic.String.Format(V.Local.fTotal,"#,###,###,##0.00####",V.Local.sTotal)
				Gui.frmXStatus.txtExtCost.Text(V.Local.sTotal)
	F.Intrinsic.Control.EndSelect

	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End

	F.Intrinsic.Control.EndTry
